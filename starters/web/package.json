{
  "name": "web-starter",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development TAMAGUI_TARGET=web vite --host --clearScreen false",
    "build": "EXTRACT=1 NODE_ENV=development TAMAGUI_TARGET=web vite build",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "serve": "npx pushstate-server -p9999 -d./dist"
  },
  "dependencies": {
    "ahpoi-one-universe-react": "^0.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.18.10",
    "tamagui": "latest"
  },
  "devDependencies": {
    "@tamagui/vite-plugin": "latest",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "vite": "^4.3.0"
  }
}

{
  "name": "ahpoi-one-universe-react",
  "version": "0.5.0",
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "src",
    "types",
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "1.30.6",
    "@tamagui/react-native-media-driver": "1.30.6",
    "@tamagui/shorthands": "1.30.6",
    "@tamagui/themes": "1.30.6",
    "@tamagui/toast": "1.30.6",
    "tamagui": "1.30.6"
  },
  "devDependencies": {
    "@tamagui/build": "1.30.6"
  },
  "publishConfig": {
    "access": "public"
  }
}

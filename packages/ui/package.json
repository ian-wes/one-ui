{
  "name": "@one-ui/react",
  "version": "0.0.0",
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "src",
    "types",
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "publish": "Anpm publish --access public",
    "pack:web": "yarn pack --out ../../starters/web/one-ui-0.3.tgz",
    "pack:expo": "yarn pack --out ../../starters/expo-app/one-ui-0.3.tgz"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "1.35.8",
    "@tamagui/react-native-media-driver": "1.35.8",
    "@tamagui/shorthands": "1.35.8",
    "@tamagui/themes": "1.35.8",
    "@tamagui/toast": "1.35.8",
    "tamagui": "1.35.8",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@tamagui/build": "1.35.8",
    "@types/react-native": "0.71.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.71.8",
    "react-native-svg": "13.4.0",
    "react-native-web": "0.18.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
